<core:FragmentDefinition  xmlns="sap.m" xmlns:f="sap.f" xmlns:core="sap.ui.core"
  xmlns:l="sap.ui.layout" xmlns:micro="sap.suite.ui.microchart">
   
    <VBox class="transparentResult" id="fragmentResult">
      <HBox class="sapUiSmallMargin" justifyContent="SpaceBetween">
        <Title id="ticket-event" text=""/>
      </HBox>
      <HBox class="sapUiSmallMargin" justifyContent="SpaceBetween">
        <Text id="ticket-date" text=""/>
      </HBox>
      <HBox class="sapUiSmallMargin" justifyContent="SpaceBetween">
        <Text text="Dein Ticket:"/>
        <Image class="barcode-scan-barcode" id="barcode-scan-barcode" />
      </HBox>
      <HBox class="sapUiSmallMargin" justifyContent="SpaceBetween">
      
        <Text text="Dein Block:"/>
        <Text id="ticket-block" text=""/>
      </HBox>
      <HBox class="sapUiSmallMargin" justifyContent="SpaceBetween">
        <Text text="Dein Eingang:"/>
        <Text id="ticket-entrance" text=""/>
      </HBox>
      <HBox class="sapUiSmallMargin" justifyContent="SpaceBetween">
        <Text text="Aktuelle Wartezeit:"/>
        <Text id="ticket-waitingTime" text="12 Minuten"/>
      </HBox>
      <HBox class="sapUiSmallMargin" justifyContent="SpaceBetween">
        <Text text="Voraussichtliche Auslastung:"/>
          <micro:ColumnMicroChart size="M" class="sapUiSmallMargin">
              <micro:columns>
                <micro:ColumnMicroChartData	id="smartChart_0" value="" color="Neutral"/>
                <micro:ColumnMicroChartData	id="smartChart_1" value="" color="Neutral"/>
                <micro:ColumnMicroChartData	id="smartChart_2" value="" color="Neutral"/>
                <micro:ColumnMicroChartData	id="smartChart_3" value="" color="Neutral"/>
                <micro:ColumnMicroChartData	id="smartChart_4" value="" color="Neutral"/>
                <micro:ColumnMicroChartData	id="smartChart_5" value="" color="Neutral"/>
                <micro:ColumnMicroChartData	id="smartChart_6" value="" color="Neutral"/>
                <micro:ColumnMicroChartData	id="smartChart_7" value="" color="Neutral"/>
              </micro:columns>
            </micro:ColumnMicroChart>
      </HBox>
      <HBox class="sapUiSmallMargin" justifyContent="SpaceBetween">
        <Text text="Perfekte Ankunftszeit:"/>
        <Text id="ticket-perfecttime" text=""/>
      </HBox>
    </VBox>

</core:FragmentDefinition >